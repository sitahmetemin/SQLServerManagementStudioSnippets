<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Alter Table Edit Column</Title>
			<Shortcut>alterEdit</Shortcut>
			<Description>Alter Table Edit Column</Description>
			<Author>Ahmet Emin ŞİT</Author>
			<SnippetTypes>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>tableName</ID>
					<Default>tableName</Default>
				</Literal>
				<Literal>
					<ID>columnName</ID>
					<Default>columnName</Default>
				</Literal>
				<Literal>
					<ID>dataType</ID>
					<Default>nvarchar(200)</Default>
				</Literal>
			</Declarations>
			<Code Language="SQL"><![CDATA[alter table $tableName$
alter column $columnName$ $dataType$
]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
